<%- include('partials/header'); -%>

    <h1>Home</h1>

    <p>
        <%= homecontent %>
    </p>

    <% newPosts.forEach(function(element) { %>
        <h1>
            <%= element.tittle %>
        </h1>
        <p>
            <%= element.postcontent.substring(0,120)+"..." %>
                <a href="/posts/<%= element._id %>">Read More</a>
        </p>

        <% }); %>


            <%- include('partials/footer'); -%>